{
  "name": "what-where",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "font-awesome": "^4.7.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "1.0.17"
  },
  "scripts": {
    "start": "PORT=3001 react-scripts start",
    "build": "PUBLIC_URL=https://movegb.github.io/what-where react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build-sprite": "svg-sprite -D src -s --ss sprite.svg --symbol-dest . ./src/sprites/*.svg",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "gh-pages": "^1.1.0",
    "react-inlinesvg": "^0.7.4",
    "svg-sprite": "^1.3.7"
  },
  "homepage": "https://movegb.github.io/what-where"
}
